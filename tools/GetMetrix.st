| metrixNestJs metrixLoopBackJs working workingNestJs workingLoopBackJs|metrixNestJs := OrderedCollection new.metrixLoopBackJs := OrderedCollection new.workingNestJs := './modeles/nestjs' asFileReference.workingLoopBackJs := './modeles/loopbackjs' asFileReference.(workingNestJs allChildren select: [ :each |     each basename endsWith: '.json' ])        do: [ :jsonFile |             | fileName modele donnees|            fileName := jsonFile path at:7.            modele := jsonFile readStreamDo: [ :stream |                 FamixTypeScriptModel new importFromJSONStream: stream ].       "Transcript show: modele; cr."	    donnees := Main new main: modele.	metrixNestJs add: donnees.        ].(workingLoopBackJs allChildren select: [ :each |     each basename endsWith: '.json' ])        do: [ :jsonFile |             | fileName modele donnees|            fileName := jsonFile path at:7.             Transcript show: fileName; cr.            modele := jsonFile readStreamDo: [ :stream |                 FamixTypeScriptModel new importFromJSONStream: stream ].	    donnees := Main new main: modele.		metrixLoopBackJs add: donnees.        ].CSVFile new generateCSV: 'nestjsResults.csv' data: metrixNestJs.CSVFile new generateCSV: 'loopbackResults.csv' data: metrixLoopBackJs.
Smalltalk snapshot: false andQuit: true.